<h2>Sign up</h2>

<%= form_with url: user_session_path, method: :post do |f| %>
  <div class="form-group">
    <%= f.label :patient_id %>
    <%= f.text_field :patient_id, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :birth_date %>
    <%= f.date_select :birth_date, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit 'ログイン', class: 'btn btn-primary' %>
  </div>
<% end %>


<%= render "devise/shared/links" %>
