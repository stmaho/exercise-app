<h2>ID/passを入力してください</h2>

<%= form_with model: @user, url: user_registration_path, method: :post do |f| %>

<div class= "contents">
  <div class="form-group">
    <%= f.text_field :patient_id, class: 'form-control', placeholder:"例) 51XXXXXX" %>
    <%= f.text_field :birth_date, class: 'form-control', placeholder:"生年月日8桁　20100101" %>
  </div>

  <div class="actions">
    <%= f.submit '新規登録', class: 'btn btn-primary' %>
  </div>
</div>
<% end %>

<%= render "devise/shared/links" %>
